{
    "name": "vsc-gsql-extension",
    "displayName": "GSQL Syntax Highlighting",
    "description": "Community driven GSQL extension for VS Code",
    "publisher": "DavidBakerEffendi",
    "version": "0.0.3",
    "icon": "images/tigergraph_128x128.png",
    "categories": [
        "Programming Languages",
        "Linters"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/DavidBakerEffendi/vsc-gsql-extension"
    },
    "license": "Apache License Version 2.0",
    "keywords": [
        "gsql",
        "tigergraph"
    ],
    "engines": {
        "vscode": "^1.0.0"
    },
    "activationEvents": [
        "onLanguage:gsql"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js"
    },
    "contributes": {
        "languages": [
            {
                "id": "gsql",
                "aliases": [
                    "GSQL",
                    "gsql"
                ],
                "extensions": [
                    ".gsql"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "gsql",
                "scopeName": "source.gsql",
                "path": "./syntaxes/gsql.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/node": "^13.13.9",
        "@types/vscode": "^1.45.0",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "js-yaml": "^3.14.0",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0",
        "axios": "^0.19.2"
    }
}
